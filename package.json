{
  "name": "nextgen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:demo": "vite --host=yardcheck.skybitz.com --port=9015",
    "dev:mock": "vite --mode mock",
    "types:watch": "tsc -b --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "build": "tsc -b && vite build",
    "lint": "eslint ./src",
    "preview": "vite preview",
    "format": "prettier --config .prettierrc --write \"**/*.{ts,tsx}\"",
    "prepare": "husky",
    "precommit": "lint-staged",
    "prepush": "tsc -b && npm run test -- --run"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.0.1",
    "@mui/lab": "^6.0.0-beta.10",
    "@mui/material": "^6.0.1",
    "@mui/x-data-grid": "^7.15.0",
    "@mui/x-date-pickers": "^7.18.0",
    "@tanstack/react-query": "^5.55.4",
    "@types/mapbox-gl": "^3.4.1",
    "apexcharts": "^4.1.0",
    "autosuggest-highlight": "^3.3.4",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "mapbox-gl": "^3.7.0",
    "moment-timezone": "^0.5.46",
    "notistack": "^3.0.1",
    "papaparse": "^5.4.1",
    "re-resizable": "^6.11.2",
    "react": "^18.3.1",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^18.3.1",
    "react-map-gl": "^7.1.7",
    "react-router-dom": "^6.26.1",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.0",
    "@types/autosuggest-highlight": "^3.2.3",
    "@types/node": "^22.9.0",
    "@types/papaparse": "^5.3.15",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "@vitest/ui": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "globals": "^15.9.0",
    "husky": "^9.1.6",
    "jsdom": "^25.0.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.4.4",
    "postcss": "^8.4.41",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "rsuite": "^5.75.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-static-copy": "^2.3.1",
    "vitest": "^2.0.5"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --config .prettierrc --write \"**/*.{ts,tsx}\""
    ]
  }
}
